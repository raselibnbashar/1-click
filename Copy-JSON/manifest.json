{
  "manifest_version": 3,
  "name": "Copy JSON",
  "version": "1.0.5",
  "description": "Extracts LinkedIn Data",
  "permissions": ["activeTab", "scripting", "downloads"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon24.png",
      "48": "icon32.png",
      "64": "icon64.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
