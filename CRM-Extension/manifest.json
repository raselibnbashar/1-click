{
  "manifest_version": 3,
  "name": "Paste JSON",
  "version": "1.0.3",
  "description": "Paste JSON One Click",
  "permissions": ["tabs", "activeTab", "clipboardRead", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
